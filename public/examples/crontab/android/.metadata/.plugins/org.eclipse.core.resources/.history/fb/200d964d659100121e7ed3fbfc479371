<project name="imported" basedir=".">
    <target name="-pre-build">
        <mkdir dir="assets/www/ampere" />
		<copy todir="assets/www/ampere">
			<fileset dir="../../../.." includes="src/**,libs/**"/>
		</copy>
    	<copy todir="assets/www">
