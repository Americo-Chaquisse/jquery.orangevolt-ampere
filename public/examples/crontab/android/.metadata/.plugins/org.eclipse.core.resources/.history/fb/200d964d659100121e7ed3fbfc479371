    	</copy>
	</target>

	<target name="-pre-clean">
		<delete dir="assets/www/ampere"/>
		<delete>
			<fileset dir="assets/www" includes="css/crontab.less,tmpl/**,crontab.js"/>
		</delete>

	</target>
</project>